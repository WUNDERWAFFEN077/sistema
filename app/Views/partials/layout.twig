								<h1>
									Dashboard
									<small>
										<i class="ace-icon fa fa-angle-double-right"></i>
										overview &amp; stats
									</small>
								</h1>
							</div>--><!-- /.page-header -->
						{% endblock %}						

						<div class="row">
							<div class="col-xs-12" id="content-block">
								<!-- PAGE CONTENT BEGINS -->
								{% block content %}
								{% endblock %}
								<!-- PAGE CONTENT ENDS -->
							</div><!-- /.col -->
						</div><!-- /.row -->

					</div><!-- /.page-content -->
				</div>
			</div><!-- /.main-content -->

			{% if menu is null or menu %}
